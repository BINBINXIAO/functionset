<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppSwipeBackTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:progressBarStyle">@android:style/Widget.Holo.ProgressBar</item>
        <item name="android:buttonStyle">@android:style/Widget.Holo.Button</item>
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/statusbar_primary</item>
        <item name="colorPrimaryDark">@color/statusbar_primarydark</item>
        <item name="colorAccent">@color/statusbar_accent</item>
    </style>

    <style name="Main_Custom_Text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18dp</item>
        <item name="android:padding">10dp</item>
    </style>
</resources>